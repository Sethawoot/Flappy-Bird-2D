# Auto detect text files and perform LF normalization
* text=auto
*.dll filter=lfs diff=lfs merge=lfs -text
*.dylib filter=lfs diff=lfs merge=lfs -text
*.so filter=lfs diff=lfs merge=lfs -text
*.db filter=lfs diff=lfs merge=lfs -text
*.sogit filter=lfs diff=lfs merge=lfs -text
add filter=lfs diff=lfs merge=lfs -text
Library/PackageCache/com.unity.burst@1.8.16/.Runtime/*.dylib filter=lfs diff=lfs merge=lfs -text
Library/PackageCache/com.unity.burst@1.8.16/.Runtime/*.dll filter=lfs diff=lfs merge=lfs -text
Library/ArtifactDB filter=lfs diff=lfs merge=lfs -text
